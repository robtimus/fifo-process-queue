{
  "name": "fifo-process-queue",
  "version": "1.2.1",
  "description": "A queue for processing data in FIFO order",
  "homepage": "https://github.com/robtimus/fifo-process-queue",
  "bugs": {
    "url": "https://github.com/robtimus/fifo-process-queue/issues"
  },
  "license": "Apache-2.0",
  "author": "Rob Spoor",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/robtimus/fifo-process-queue.git"
  },
  "scripts": {
    "lint": "eslint index.js",
    "format": "prettier --write \"**/*.+(json|js|jsx|ts|tsx)\"",
    "format:check": "prettier --check \"**/*.+(json|js|jsx|ts|tsx)\"",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "eslint": "^8.57.1",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ]
  },
  "prettier": {
    "printWidth": 150
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": {
            "esModuleInterop": false
          },
          "diagnostics": {
            "ignoreCodes": [
              "151001"
            ]
          }
        }
      ]
    },
    "collectCoverage": true
  }
}
